{
  "name": "deep-fy-test1",
  "version": "7.0.0",
  "description": "Components Library for deep light",
  "keywords": [
    "code-editor"
  ],
  "author": "jingpengju",
  "license": "BSI",
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --fix",
    "test": "jest",
    "test:c": "jest --coverage",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "format": "prettier --write packages/",
    "start": "npm run dev",
    "dev": "vite",
    "build": "vite build",
    "major": "npm version major -m 'build: update major'",
    "minor": "npm version minor -m 'build: update minor'",
    "patch": "npm version patch -m 'build: update patch'",
    "pub": "npm run build && cd dist/ && npm publish --access=public",
    "pub:major": "npm run major && npm run build && cd dist/ &&npm publish --access=public",
    "pub:minor": "npm run minor && npm run build && cd dist/ && npm publish --access=public",
    "pub:patch": "npm run patch && npm run build && cd dist/ && npm publish --access=public",
    "unpub": "npm unpublish deep-light --force"
  },
  "dependencies": {
    "sortablejs": "^1.15.0",
    "uuid": "^9.0.0",
    "vue": "^3.3.4",
    "vxe-table": "^4.3.9"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/sortablejs": "^1.15.1",
    "@types/uuid": "^9.0.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "jest": "^29.6.1",
    "node-sass": "^9.0.0",
    "prettier": "^2.8.8",
    "sass": "^1.64.1",
    "sass-loader": "^13.3.2",
    "typescript": "~5.0.4",
    "unplugin-vue-define-options": "^1.3.11",
    "vite": "^4.3.9",
    "vite-eslint-plugin": "^1.9.2",
    "vite-plugin-compression2": "^0.10.3",
    "vite-plugin-css-injected-by-js": "^3.2.1",
    "vite-plugin-dts": "^3.3.1",
    "vitest": "^0.32.0",
    "vue-tsc": "^1.6.5"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "not dead"
  ]
}
